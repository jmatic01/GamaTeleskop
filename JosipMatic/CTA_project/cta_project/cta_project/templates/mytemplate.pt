<script>
$('#search').change(filter).keyup(filter)
function filter(){
	var str = $('#search').val();
	if(str!=""){
		$('#data tr:gt(0)').hide();
		$('#data tr:gt(0)').each(function(){
			var tr = $(this);
			if( tr.html().indexOf(str)>1)
			{
				tr.stop().show();
			} 
		})
	} else{
		$('#data tr:gt(0)').show()
	}
}

